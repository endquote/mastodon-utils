{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "sync-feeds",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/dev",
      "args": [
        "sync-feeds"
      ],
      "console": "integratedTerminal",
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "sync-stars",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/dev",
      "args": [
        "sync-stars"
      ],
      "console": "integratedTerminal",
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "timeline - home",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/dev",
      "args": [
        "timeline",
        "-f",
        "home",
        "-p",
        "~/Downloads/mastodon.xml"
      ],
      "console": "integratedTerminal",
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "timeline - public",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/dev",
      "args": [
        "timeline",
        "-f",
        "public"
      ],
      "console": "integratedTerminal",
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "timeline - tag",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/bin/dev",
      "args": [
        "timeline",
        "-f",
        "tag",
        "-t",
        "mastodon"
      ],
      "console": "integratedTerminal",
      "outputCapture": "std"
    }
  ]
}
